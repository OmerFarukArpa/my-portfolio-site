
@model MvcCv.Models.Entity.TblSertifikalarım


@{
    ViewBag.Title = "SertifikaGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

 

<h2>Sertifika Düzenleme Sayfası</h2>

<br />
<form method="post">

    @Html.Label("Sertifika Açıklaması")
    @Html.TextBoxFor(x => x.Aciklama, new { @class = "form-control" })
    <br />
    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })

    <br />
    <br />

    <button class="btn btn-success">Güncelle</button>
    <a href="/Certificate/RemoveCertificate/@ViewBag.d" class="btn btn-danger">Sil</a>
</form>

